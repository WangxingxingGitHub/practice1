//                                                    while循环语句
#include<stdio.h>
int main()
{
	int i = 0;
	while(i<10)
	{
		if(5 == i)
		{
			//continue;//break;
		}
		printf("%d ",i);
		i++;
	}
	return 0;
}

//getcha和putchar的使用
//getchar获取一个字符存入到变量中
//putchar把获取的字符打印输出
//输入：ctrl+z+回车 getchar获得一个EOF
#include<stdio.h>
int main()
{
	int ch = 0;
	while((ch=getchar()) != EOF)
	{
		putchar(ch);
	}
	return 0;
}

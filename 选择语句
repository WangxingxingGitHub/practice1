//                                                           if选择语句
#include<stdio.h>
int main()
{
	int age = 0;
	scanf("%d",&age);
	if(age<18)
		printf("未成年\n");
	else if(age>=18 && age<=28)
		printf("青年\n");
	else
		printf("中年\n");
	return 0;
}

#include<stdio.h>
int main()
{
	int age = 0;
	scanf("%d",&age);
	if(age<18)
		printf("未成年\n");
	else 
	{
	   if(age>=18 && age<=28)
	       printf("青年\n");
       else
	       printf("中年\n");
	}
	return 0;
}

//注意书写规范，= ：赋值  == ：判断相等 
#include<stdio.h>
int main()
{
	int num = 0;
	if(5 = num)
	{
		printf("hehe\n");
	}
	return 0;
}

